{
  "version": 2,
  "builds": [
    {
      "src": "dist/estudio_style/server/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/estudio_style/server/server.mjs"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/estudio_style/browser"
}
